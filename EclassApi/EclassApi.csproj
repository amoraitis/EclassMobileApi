<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <id>EclassApi</id>
    <version>1.1.6</version>
    <title>EclassApi</title>
    <authors>amoraitis</authors>
    <owners>amoraitis</owners>
    <projectUrl>https://github.com/amoraitis/EclassMobileApi</projectUrl>
    <PublishUrl>https://github.com/amoraitis/EclassMobileApi</PublishUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An Eclass Mobile API Client for C#</description>
    <releaseNotes>Bug fixes and improvements</releaseNotes>
    <copyright>Copyright 2017</copyright>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>lib\netcoreapp2.0\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>lib\netcoreapp2.0\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flurl.Http">
		<Version>2.0.1</Version>
	</PackageReference>
    <PackageReference Include="HtmlAgilityPack">
		<Version>1.6.5</Version>
	</PackageReference>
    <PackageReference Include="xwebrss" Version="2.1.0" />
  </ItemGroup>

</Project>
